@using MVCBootsrap.Models.EntityFramework
@model List<Order>
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Contact</h2>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Tarix</th>
            <th>ID</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var ordr in Model)
            {
                <tr>
                    <td>@ordr.OrderDate</td>
                    <td>@ordr.OrderID</td>
                </tr>
            }
    </tbody>
</table>

